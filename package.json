  "description": "Starter project for Next-gen FHIR Apps",
  "keywords": [
    "FHIR",
    "React",
    "Redux",
    "Redux-Saga",
    "Webpack",
    "ES6"
  ],
  "author": "Asymmetrik Ltd.",
  "contributors": [
    "Shane O'Neill <soneill@asymmetrik.com>"
  ],
  "bugs": "https://github.com/asymmetrik/fhir-app-starter/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asymmetrik/fhir-app-starter"
  },
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "connected-react-router": "^6.8.0",
    "fhirclient": "^2.3.1",
    "history": "^4.10.1",
    "immer": "^7.0.7",
    "lodash": "^4.17.19",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.1.1"
  },
  "scripts": {
    "start": "yarn build:launch && react-scripts start",
    "build": "yarn build:launch && react-scripts build",
    "build:launch": "node ./internals/build.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint  --ext .js --ext .jsx src",
    "format": "prettier \"**/*.{js,jsx}\" --write",
    "changelog": "conventional-changelog -p angular -s -i CHANGELOG.md"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "handlebars": "^4.7.6",
    "prettier": "^2.0.5"
  }
}
